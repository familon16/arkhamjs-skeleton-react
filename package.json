{
  "name": "arkhamjs-skeleton",
  "version": "2.0.1",
  "description": "ArkhamJS Skeleton",
  "license": "MIT",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "arkhamjs",
    "nitrogenlabs",
    "react",
    "skeleton",
    "react-router",
    "express"
  ],
  "authors": [
    "Giraldo Rosales <giraldo@nitrogenlabs.com> (http://nitrogenlabs.com/)"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/nitrogenlabs/arkhamjs-skeleton"
  },
  "homepage": "https://github.com/nitrogenlabs/arkhamjs-skeleton",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/arkhamjs-skeleton/issues"
  },
  "engines": {
    "node": ">7.10.0",
    "npm": ">4.2.0"
  },
  "scripts": {
    "development": "NODE_ENV=development gulp",
    "docs": "gulp docs",
    "compile": "NODE_ENV=preprod npm test && gulp compile",
    "production": "NODE_ENV=production npm test && gulp production",
    "start": "npm run development",
    "test": "NODE_ENV=test gulp test && jest"
  },
  "dependencies": {
    "arkhamjs": "2.0.6",
    "bluebird": "^3.5.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.7",
    "@types/history": "^4.6.0",
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.28",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-router-dom": "^4.0.7",
    "autoprefixer": "^7.1.4",
    "awesome-typescript-loader": "^3.2.3",
    "body-parser": "^1.18.1",
    "connect-history-api-fallback": "^1.3.0",
    "del": "^3.0.0",
    "enzyme": "^2.9.1",
    "exports-loader": "^0.6.4",
    "express": "^4.15.4",
    "express-useragent": "^1.0.8",
    "fs": "0.0.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-exec": "^2.1.3",
    "gulp-flatten": "^0.3.1",
    "gulp-image": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^3.1.0",
    "gulp-svgmin": "^1.2.4",
    "gulp-svgstore": "^6.1.0",
    "gulp-tslint": "^8.1.2",
    "gulp-unzip": "^0.2.0",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^4.0.0",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jest": "^21.1.0",
    "json-d-ts": "^1.0.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "react-addons-test-utils": "^15.6.0",
    "react-scripts-ts": "^2.7.0",
    "react-test-renderer": "^15.6.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.5.0",
    "regenerator-runtime": "^0.11.0",
    "require-dir": "^0.3.2",
    "run-sequence": "^2.1.0",
    "source-map-loader": "^0.2.1",
    "source-map-support": "^0.4.18",
    "ts-jest": "^21.0.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.2.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.5.2",
    "vinyl-paths": "^2.1.0",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "yargs": "^9.0.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "html",
      "text"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^actions/(.*)": "<rootDir>/src/actions/$1",
      "^components/(.*)": "<rootDir>/src/components/$1",
      "^constants/(.*)": "<rootDir>/src/constants/$1",
      "^errors/(.*)": "<rootDir>/src/errors/$1",
      "^services/(.*)": "<rootDir>/src/services/$1",
      "^stores/(.*)": "<rootDir>/src/stores/$1",
      "^views/(.*)": "<rootDir>/src/views/$1"
    },
    "setupFiles": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "src/.*\\.test\\.(ts|tsx|js)$",
    "testURL": "http://localhost",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "verbose": false
  }
}
